{
  "name": "@create-uni/monorepo",
  "type": "module",
  "version": "1.7.3",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "description": "轻松创建你的 uni-app 项目",
  "author": "FliPPeDround <flippedround@qq.com>",
  "license": "MIT",
  "funding": "https://afdian.com/a/flippedround",
  "homepage": "https://github.com/uni-helper/create-uni#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/create-uni.git"
  },
  "bugs": "https://github.com/uni-helper/create-uni/issues",
  "keywords": [
    "uni",
    "uni-app",
    "create-uni",
    "cli",
    "vue",
    "miniapp",
    "create"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "typecheck": "tsc --noEmit",
    "up": "cd ./template && pnpx taze@latest major -w -r && cd ./base && pnpx @dcloudio/uvm@latest --manager pnpm"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/ejs": "^3.1.5",
    "@types/envinfo": "^7.8.4",
    "@types/minimist": "^1.2.5",
    "@types/node": "^18.19.50",
    "@types/prompts": "^2.4.9",
    "@uni-helper/eslint-config": "^0.2.0",
    "bumpp": "^9.5.2",
    "ejs": "^3.1.10",
    "envinfo": "^7.13.0",
    "eslint": "^9.10.0",
    "json5": "^2.2.3",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.10",
    "minimist": "^1.2.8",
    "path-exists": "^5.0.0",
    "prompts": "^2.4.2",
    "simple-git-hooks": "^2.11.1",
    "terser": "^5.31.6",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
