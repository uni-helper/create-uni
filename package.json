{
  "name": "create-uni",
  "type": "module",
  "version": "0.1.6",
  "packageManager": "pnpm@8.10.3",
  "description": "An easy way to start a uni-app project",
  "author": "FliPPeDround <flippedround@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/uni-helper/create-uni#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/create-uni.git"
  },
  "bugs": "https://github.com/uni-helper/create-uni/issues",
  "keywords": [
    "uni",
    "uni-app",
    "create-uni",
    "cli",
    "vue",
    "miniapp"
  ],
  "sideEffects": false,
  "bin": {
    "create-uni": "outfile.cjs"
  },
  "files": [
    "outfile.cjs",
    "template"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "node outfile.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.9",
    "@antfu/utils": "^0.7.6",
    "@types/ejs": "^3.1.5",
    "@types/minimist": "^1.2.5",
    "@types/node": "^18.18.9",
    "@types/prompts": "^2.4.8",
    "@uni-helper/eslint-config": "^0.0.4",
    "bumpp": "^9.2.0",
    "ejs": "^3.1.9",
    "esbuild": "^0.18.10",
    "eslint": "^8.53.0",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.1.0",
    "minimist": "^1.2.8",
    "pnpm": "^8.10.3",
    "prompts": "^2.4.2",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
